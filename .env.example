# TextLink SMS API
TEXTLINK_API_KEY=your_api_key_here
TEXTLINK_WEBHOOK_SECRET=your_webhook_secret_here

# OpenClaw gateway (for routing incoming SMS to the agent)
OPENCLAW_GATEWAY_URL=http://127.0.0.1:18789
OPENCLAW_HOOKS_TOKEN=clawbot-hook-secret

# ElevenLabs (voice notes for SMS — female voice for Natalie)
# Get key: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Google OAuth2 Configuration
# Get these from https://console.cloud.google.com/apis/credentials
# For LOCAL DEVELOPMENT on Mac Mini, use:
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:8000/auth/callback
# Note: Make sure this exact URI is added to your Google OAuth client's authorized redirect URIs

# Token Cache Configuration
# Use "file" for file-based caching or "redis" for Redis caching
TOKEN_CACHE_TYPE=file
TOKEN_CACHE_PATH=./.token_cache

# Redis Configuration (only needed if TOKEN_CACHE_TYPE=redis)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Multi-Agent Configuration
ENABLE_MULTI_AGENT=true
AGENT_ROUTING_STRATEGY=intent_based  # Options: round_robin, load_balance, intent_based, random

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Natalie (Equestro Labs) email — DreamHost send/receive for natalie@equestrolabs.com
# Used by OpenClaw and lead-gen when user_id is this address (no Google OAuth needed for email).
NATALIE_EMAIL=natalie@equestrolabs.com
NATALIE_EMAIL_PASSWORD=your_dreamhost_email_password
# Optional; defaults shown (DreamHost)
# NATALIE_SMTP_HOST=smtp.dreamhost.com
# NATALIE_SMTP_PORT=587
# NATALIE_IMAP_HOST=imap.dreamhost.com
# NATALIE_IMAP_PORT=993

# Lead Generation Pipeline (Equestro Labs)
# Use natalie@equestrolabs.com so outreach and OpenClaw send/receive from this address.
LEAD_GEN_GMAIL_USER=natalie@equestrolabs.com
LEAD_GEN_SHEET_ID=your_google_sheet_id
# Optional: Google account that owns the Sheet (if different from NATALIE_EMAIL). Authorize at /auth/authorize.
# LEAD_GEN_SHEET_USER=your_google@gmail.com
CLAWBOT_BASE_URL=http://localhost:8000
RAPIDAPI_LEAD_KEY=your_rapidapi_key
RAPIDAPI_PEOPLE_KEY=your_rapidapi_people_key
# E2E test: LEAD_GEN_DRY_RUN=1 to skip real SMS/email; TEST_LEAD_PHONE for --inject live run

# Website Audit (Equestro Labs)
# Served on Render at /audit/dashboard (e.g. https://websites-natalie.onrender.com/audit/dashboard).
# Website Customers registry uses the same sheet as lead-gen (LEAD_GEN_SHEET_ID), tab "Website Customers".
# Optional: for future "where we can get you" PageSpeed scores.
# PAGESPEED_API_KEY=your_google_api_key
